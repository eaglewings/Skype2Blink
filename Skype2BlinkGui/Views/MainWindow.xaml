<Window x:Name="Skype2Blink" x:Class="Skype2BlinkGui.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Skype2BlinkGui"
        mc:Ignorable="d"
        Title="Skype2Blink Status" Height="350" Width="525">
    <StackPanel Orientation="Vertical" VerticalAlignment="Top">
        <StackPanel Orientation="Horizontal" VerticalAlignment="Top">
            <Label>Skype running</Label>
            <Label Content="{Binding SkypeState.Running}"></Label>
        </StackPanel>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Top">
            <Label>Skype windows</Label>
            <Label></Label>
        </StackPanel>
        
    </StackPanel>
</Window>
